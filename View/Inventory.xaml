<UserControl x:Class="SystemForGasEguipment.View.Inventory"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
              xmlns:vm="clr-namespace:SystemForGasEguipment.ViewModel" 
             xmlns:local="clr-namespace:SystemForGasEguipment.View"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="622"
             Style="{StaticResource Page_Style}">
    <UserControl.DataContext>
        <vm:InventoryVM/>
    </UserControl.DataContext>
    <Grid>
        <Button x:Name="buttonInventory" PreviewMouseDown="buttonInventory_PreviewMouseDown" Content="Отчет по инвенторизации"  HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="580" Height="36" Background="#FF305FDC" BorderBrush="{x:Null}" Foreground="White" />

    </Grid>
</UserControl>